import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.SQLException;
import java.sql.Statement;
import java.util.UUID;

public class MusicTester {

	public static void main(String[] args) {
		Song s1 = new Song("1f196bbd-752f-4003-96c2-e39bbb0f8693");
			System.out.println(s1.getTitle());
			System.out.println(s1.getLength());
		Song s = new Song("FlatBed Freestyle", 3, "2018-05-11 00:00:00", "2017-12-30 00:00:00");
			System.out.println(s.getSongID());
			System.out.println(s.getTitle());
			System.out.println(s.getLength());
			System.out.println(s.getReleaseDate());
			System.out.println(s.getRecordDate());
			System.out.println(s.getFilePath());
	}

}
